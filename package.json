{
  "name": "react-context-x",
  "description": "Optimize shared states in React (353 B)",
  "version": "0.3.1",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "files": [
    "dist/**/*"
  ],
  "exports": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.js"
  },
  "engines": {
    "node": ">=15",
    "npm": ">=7"
  },
  "scripts": {
    "prod": "node scripts/node-prod && gzip -c dist/index.mjs | wc -c",
    "dev": "node scripts/node-dev",
    "watch": "watchman-make -p 'src/**/*' --run \"npm run dev\"",
    "prepare": "npm run prod"
  },
  "peerDependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "esbuild": "^0.8.41",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "prettier": "^2.2.1"
  }
}
